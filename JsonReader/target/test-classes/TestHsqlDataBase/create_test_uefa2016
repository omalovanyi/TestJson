CREATE TABLE "UEFA2016" (
name VARCHAR(60),
bio VARCHAR(1500),
is_photo VARCHAR(3),
is_spec_player VARCHAR(100),
position VARCHAR(50),
number_player NUMERIC,
caps NUMERIC,
cnt_goals NUMERIC,
club VARCHAR(100),
league VARCHAR(100),
date_birthday DATE,
rating_match1 VARCHAR(100),
rating_match2 VARCHAR(100),
rating_match3 VARCHAR(100)
);