CREATE TABLE UEFA2016 (
  name VARCHAR2(60),
  bio VARCHAR2(1500),
  is_photo VARCHAR2(3),
  is_spec_player VARCHAR2(100),
  position VARCHAR2(50),
  number_player NUMBER,
  caps NUMBER,
  cnt_goals NUMBER,
  club VARCHAR2(100),
  league VARCHAR2(100),
  date_birthday DATE,
  rating_match1 VARCHAR2(100),
  rating_match2 VARCHAR2(100),
  rating_match3 VARCHAR2(100)
  );